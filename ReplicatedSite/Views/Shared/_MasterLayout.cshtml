                <div class="container">
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        @if (!GlobalSettings.Globalization.HideForLive)
                        {
                    <!-- Site Links -->
                        <ul class="nav navbar-nav">
                            @{
                                    var excludedCategoryIDs = new[] { "account" };
                                    var nodes = Settings.SiteMap.Current.Items.Where(c => c.IsVisible());

                                    if (Identity.Customer == null)
                                    {
                                        nodes = nodes.Where(c => !excludedCategoryIDs.Contains(c.ID));
                                    }
                            }
                            <!-- Dropdown Links -->
                            @foreach (var node in nodes.Cast<NavigationSiteMapNode>())
                                {
                                    if (node.Children.Any())
                                    {
                                <li class="dropdown dropdown-hover @node.DeviceVisibilityCssClass">
                                    <a class="dropdown-toggle" data-navigation="@node.ActiveGroup" data-toggle="dropdown">
                                        @node.Label <b class="caret"></b>
                                    </a>
                                    <ul class="dropdown-menu col-2">
                                        @foreach (var item in node.Children.Where(c => c.IsVisible()))
                                                {
                                                    // Dividers
                                                    var dividerItem = item as DividerNode;
                                                    if (dividerItem != null)
                                                    {
                                                <li class="divider"></li>

                                                        continue;
                                                    }

                                                    // Headings
                                                    var headingItem = item as HeadingNode;
                                                    if (headingItem != null)
                                                    {
                                                <li role="presentation" class="dropdown-header">@headingItem.Label</li>

                                                        continue;
                                                    }

                                                    // Links
                                                    var linkItem = item as NavigationSiteMapNode;
                                                    if (linkItem != null)
                                                    {
                                                <li class="@linkItem.DeviceVisibilityCssClass @(linkItem.IsActive() ? "active" : "")">
                                                    <a href="@linkItem.Url" target="@linkItem.Target">
                                                        @if (linkItem.HasIcon)
                                                                {
                                                                <i class="@linkItem.Icon"></i>
                                                                }
                                                        @item.Label
                                                    </a>
                                                </li>
                                                    }
                                                }
                                    </ul>
                                </li>
                                    }
                                    else
                                    {
                                <!-- Non-Dropdown Links -->
                                <li class="@node.DeviceVisibilityCssClass">
                                    <a href="@node.Url" data-navigation="@node.ActiveGroup">
                                        @if (node.HasIcon)
                                                {
                                                <i class="@node.Icon"></i>
                                                }
                                        @node.Label
                                    </a>
                                </li>
                                    }
                                }
                        </ul>
                                    }
                        <!-- Social Media Icons -->
                        <div id="social-media-container" class="hidden-xs">
                            <ul class="list-unstyled list-inline">
                                <li>
                                    <a href="@GlobalSettings.Company.Facebook" target="_blank" class="id-social">
