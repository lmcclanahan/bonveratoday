    @* Language Modal *@
    <div class="modal fade" id="language" tabindex="-1" role="dialog" aria-labelledby="language" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title" id="myModalLabel">@Resources.Common.SelectCountLang</h4>
                </div>
                <div class="modal-body">
                    <p>@Resources.Common.SelectCountry</p>
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="control-group">
                                <label class="control-label" for="Country">@Resources.Common.ChooseCountry</label>
                                <div class="controls">
                                    @{
                                        var availableMarkets = GlobalSettings.Markets.AvailableMarkets;
                                        var selectedCountryCode = GlobalUtilities.GetSelectedCountryCode();
                                        var selectedMarket = availableMarkets.FirstOrDefault(c => c.Countries.Contains(selectedCountryCode));
                                    }
                                    <select name="Country" id="CountrySelector" class="span12 form-control">
                                        @foreach (var item in availableMarkets)
                                        {
                                            var selected = (selectedMarket == item) ? "selected='selected'" : "";
                                        <option value="@item.Countries.FirstOrDefault()" @selected>@item.Description</option>
                                        }
                                    </select>
                                </div>
                                <!-- /controls -->
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="control-group">

                                <label class="control-label" for="Country">@Resources.Common.ChooseLang</label>
                                <div class="controls">
                                    <select name="Language" class="span12 form-control">
                                        @foreach (var lang in GlobalSettings.Globalization.AvailableLanguages)
                                        {
                                        <option value="@lang.CultureCode" selected="">@CommonResources.Languages(lang.LanguageID)</option>
                                        }
                                    </select>
                                </div>
                                <!-- /controls -->
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <a class="btn btn-primary" id="languageSubmit">@Resources.Common.Submit</a>
                        </div>
                    </div>
                    <div class="space-30"></div>
                </div>
            </div>
        </div>
    </div>

