@model MyTeamViewModel




<div class="panel panel-default">
    <div class="dashboard-heading" role="tab" id="headingOne">
        <h4 class="panel-title">
            <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                @Model.CurrentTeamMember.FirstName @Model.CurrentTeamMember.LastName  @Model.CurrentTeamMember.HighestAchievedRank.RankDescription
            </a>
        </h4>
    </div>
    <div id="collapseOne" class="panel-collapse collapse in " role="tabpanel" aria-labelledby="headingOne">
        <div class="panel-body">
            <div class="row margin-10">
                <div class="col-xs-10">
                    <h4 style="margin-top: 0px; color: black;">Bonvera ID# @Model.CurrentTeamMember.CustomerID</h4>
                    <div class="row margin-10">
                        <div class="col-sm-6">
                            <label>PBV: </label>
                            <span>@Model.CurrentTeamMemberPBV.ToString("0.00")</span>
                            <div class="space-10"></div>
                            <label>PCBV: </label>
                            <span>@Model.CurrentTeamMemberPCBV.ToString("0.00")</span>
                        </div>
                        <div class="col-sm-6">
                            <label>TGBV </label>
                            <span>@Model.CurrentTeamMemberTGBV.ToString("0.00")</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
